{
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "project_panel": {
    "hide_hidden": true
  },
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "icon_theme": "Catppuccin Frapp√©",
  "theme": {
    "mode": "dark",
    "light": "Catppuccin Latte",
    "dark": "Tokyo Night Moon"
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "catppuccin-icons": true,
    "nix": true,
    "ron": true,
    "toml": true,
    "cargo-tom": true
  },
  "disable_ai": true,
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "always_treat_brackets_as_autoclosed": true,
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "diagnostics": {
          "disabled": ["trait-impl-incorrect-safety"]
        }
      }
    }
  }
}
