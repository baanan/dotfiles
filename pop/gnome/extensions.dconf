[aylurs-widgets]
background-clock=false
battery-bar=false
dash-board=true
dash-button-enable=true
dash-button-icon-path='/usr/share/icons/Pop/scalable/actions/view-app-grid-symbolic.svg'
dash-button-label=''
dash-button-offset=0
dash-button-position=0
dash-button-show-icon=true
dash-hide-activities=false
dash-links-names=['reddit', 'youtube', 'gmail', 'twitter', 'github']
dash-links-urls=['https://www.reddit.com/', 'https://www.youtube.com/', 'https://www.gmail.com/', 'https://twitter.com/', 'https://www.github.com/']
