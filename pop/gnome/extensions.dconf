hacks-level=3

[blur-my-shell/applications]
whitelist=['Gnome-terminal', 'kitty']

[blur-my-shell/dash-to-dock]
blur=false

[blur-my-shell/panel]
blur=true
brightness=0.5
customize=true
noise-amount=0.0
noise-lightness=0.0
override-background=false
sigma=125
static-blur=false
unblur-in-overview=true

[dash-to-dock]
manualhide=true
show-mounts=false

[dash-to-panel]
animate-appicon-hover=false
animate-appicon-hover-animation-extent={'RIPPLE': 4, 'PLANK': 4, 'SIMPLE': 1}
